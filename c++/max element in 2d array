#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int m,n,i,j,a[10][10];
	cin>>m;
	cin>>n;
	int max;
	for(i=0;i<m;i++)
	{
	    for(j=0;j<n;j++)
	    {
	        cin>>a[i][j];
	    }
	}
	max=a[0][0];
	for(i=0;i<m;i++)
	{
	    for(j=0;j<n;j++)
	    {
	        if(a[i][j]>max)
	        {
	            max=a[i][j];
	        }
	    }
	}
	cout<<max;
	
	return 0;
}
