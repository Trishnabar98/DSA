#include <iostream>
using namespace std;

int vowel(char ch)
{
    if(ch=='a'|| ch=='e'||ch=='i'||ch=='o'||ch=='u'||ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U')
        return 1;
    else
        return 0;
}
int main() {
	// your code goes here
	string s;
	getline(cin,s);
	for(int i=0;s[i]!='\0';i++)
	{
	    if(vowel(s[i])==0)
	    {
	        cout<<s[i];
	    }
	}
	return 0;
}
