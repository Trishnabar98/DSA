#include <iostream>
using namespace std;

int main() {
	int n,i,j,t;
	cin>>n;
	int a[n];
	for(i=0;i<n;i++)
	{
	    cin>>a[i];
	}
	for(i=0;i<n/2;i++)
	{
	    for(j=0;j<n/2;j++)
	    {
	        if(a[i]<a[j])
	        {
	            t=a[i];
	            a[i]=a[j];
	            a[j]=t;
	        }
	    }
	}
	for(i=n/2;i<n;i++)
	{
	    for(j=n/2;j<n;j++)
	    {
	        if(a[i]>a[j])
	        {
	            t=a[i];
	            a[i]=a[j];
	            a[j]=t;
	        }
	    }
	}
	
	cout<<"1st half:-"<<endl;
	for(i=0;i<n/2;i++)
	{
	    cout<<a[i]<<" ";
	}
	cout<<"\n2nd half:-"<<endl;
		for(i=n/2;i<n;i++)
	{
	    cout<<a[i]<<" ";
	}
	
	
	return 0;
}
